<!DOCTYPE html>
<html lang="en">

<head>

    <!-- important meta tags -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width-device-width, initial-scale=1">

    <!-- Title -->
    <title>@ViewBag.Title</title>

    <!-- Favicon -->
    <link rel="Icon" href="/Content/img/favicon/favicon.ico">

    !-- Open Sans Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;700&display=swap" rel="stylesheet">

    <!-- Font Awesome -->
    @*<link rel="stylesheet" href="/content/css/font-awesome/css/font-awesome.min.css">*@
    @Styles.Render("~/content/Admin/css/font-awesome/css/font-awesome.min.css")
    

    <!-- Bootstrap CSS -->
    @Styles.Render("~/Content/css/bootstrap/bootstrap.min.css")

    <!-- Custome CSS -->
    @Styles.Render("~/Content/Admin/css/style.css")

    <!-- Responsive CSS -->
